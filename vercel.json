{
  "version": 2,
  "routes": [
    {
      "src": "/assets/(.*)",
      "dest": "/Assets/$1",
      "caseSensitive": false
    },
    {
      "src": "/css/(.*)",
      "dest": "/css/$1",
      "caseSensitive": false
    },
    {
      "src": "/js/(.*)",
      "dest": "/js/$1",
      "caseSensitive": false
    },
    {
      "handle": "filesystem"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ]
}